FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  LES;


RAS
{
    model    RNGkEpsilon;

    turbulence      on;
    printCoeffs     on;
}


LES
{
  model Smagorinsky;
  turbulence on;
  delta cubeRootVol;
  cubeRootVolCoeffs
    {
      deltaCoeff 1;
    }
}


