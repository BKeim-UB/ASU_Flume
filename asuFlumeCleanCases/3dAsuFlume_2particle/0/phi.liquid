FoamFile
{
    format      binary;
    class       surfaceScalarField;
    location    "0";
    object      phi.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    top
    {
        type            calculated;
        value           uniform 0;
    }
    groundAndWall
    {
        type            fixedValue;
        value           uniform 0;
    }
}


