FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      kineticTheory:nus.particles1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    top
    {
        type            calculated;
        value           uniform 0;
    }
    groundAndWall
    {
        type            calculated;
        value           uniform 0;
    }
}


